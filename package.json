{
  "name": "options-bot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "IBKR paper-first AI options decision engine (TypeScript + Bun)",
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "dev:api": "bun --watch src/server.ts",
    "start": "bun run ui:build && bun src/server.ts",
    "ui:dev": "vite --config web/vite.config.js",
    "ui:build": "vite build --config web/vite.config.js",
    "test": "bun test",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@fastify/cors": "^10.0.1",
    "@stoqey/ib": "^1.5.3",
    "@tanstack/react-query": "^5.90.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fastify": "^5.2.1",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.13.5",
    "@vitejs/plugin-react": "^5.1.3",
    "bun-types": "^1.3.9",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.3",
    "vite": "^7.3.1"
  }
}
